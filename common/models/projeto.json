{
  "name": "Projeto",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "dataInicio": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "dataTermino": {
      "type": "date"
    },
    "limiteParticipantes": {
      "type": "number"
    },
    "atualParticipantes": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "ativo": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "resumo": {
      "type": "string"
    },
    "requisitos": {
      "type": "string"
    },
    "tipo": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "docente": {
      "type": "belongsTo",
      "model": "Docente",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": "SubareaId",
      "options": {
        "nestRemoting": true
      }
    },
    "subarea": {
      "type": "belongsTo",
      "model": "Subarea",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "coorientador": {
      "type": "belongsTo",
      "model": "Docente",
      "foreignKey": "coorientadorId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "Docente",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
