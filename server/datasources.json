{
  "mem": {
    "name": "mem",
    "connector": "memory"
  },
  "db": {
    "host": "localhost",
    "port": 3306,
    "database": "${DS}",
    "password": "${DSPASSWORD}",
    "name": "db",
    "connector": "mysql",
    "user": "root"
  },
  "email": {
    "name": "email",
    "connector": "mail",
    "transports": [
      {
        "type": "SMTP",
        "host": "smtp.gmail.com",
        "secure": true,
        "port": 465,
        "auth": {
          "user": "${MAIL}",
          "pass": "${MAILPASSWORD}"
        }
      }
    ]
  }
}
